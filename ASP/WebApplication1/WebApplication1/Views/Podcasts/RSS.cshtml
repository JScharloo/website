@{Layout = null;}
@model IEnumerable<Podcast>

<?xml version="1.0" encoding="utf-8" ?>
<rss version="2.0">
    <channel>
        <title>RSS feed leerwerkbedrijf Lisa</title>
        <description>Hier komt de beschrijving van de RSS feed van Lisa.</description>
        <link>http://www.leerwerkbedrijflisa.nl</link>

        @foreach(var podcast in Model)
        {
            <item>
                <title>@podcast.Title</title>
                <description>@podcast.Content</description>
                <link>Hier komt een link test </link>
            </item>
        }
    </channel>
</rss>